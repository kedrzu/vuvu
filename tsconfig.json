{
    "compileOnSave": false,
    "compilerOptions": {
        "module": "commonjs",
        "lib": ["es2015", "dom"],
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "outDir": "./types",
        "target": "es6",
        "baseUrl": ".",
        "mapRoot": ".",
        "sourceMap": true,
        "declaration": true,
        "paths": {
            "vuvu/*": ["src/*"],
            "vuvu": ["src"]
        }
    }
}
